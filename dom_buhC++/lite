CREATE TABLE IF NOT EXISTS accounts (id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT);CREATE TABLE IF NOT EXISTS debet_category(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT);CREATE TABLE IF NOT EXISTS debet_type(    id INTEGER PRIMARY KEY AUTOINCREMENT,     FOREIGN KEY (category_id) REFERENCES debet_category(id),     name TEXT);CREATE TABLE IF NOT EXISTS debet    (    id INTEGER PRIMARY KEY AUTOINCREMENT,    FOREIGN KEY (account_id) REFERENCES accounts(id),     FOREIGN KEY (debet_type_id) REFERENCES debet_type(id),    summ INTEGER,    name TEXT);CREATE TABLE IF NOT EXISTS credit_category(id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT);CREATE TABLE IF NOT EXISTS credit_type(    id INTEGER PRIMARY KEY AUTOINCREMENT,     FOREIGN KEY (category_id) REFERENCES debet_category(id),     name TEXT);CREATE TABLE IF NOT EXISTS credit    (    id INTEGER PRIMARY KEY AUTOINCREMENT,    FOREIGN KEY (account_id) REFERENCES accounts(id),     FOREIGN KEY (credit_type_id) REFERENCES credit_type(id),    summ INTEGER,    name TEXT );